<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="573.0" prefWidth="853.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.qrcode.Create">
    <children>
        <Text layoutX="297.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Generate QR code!">
            <font>
                <Font name="Arial Bold" size="27.0" />
            </font>
            <effect>
                <DropShadow>
                    <color>
                        <Color red="1.0" green="0.4988333284854889" blue="0.029999999329447746" opacity="0.8756345510482788" />
                    </color>
                </DropShadow>
            </effect>
        </Text>
        <Button fx:id="go" onAction="#setGo" layoutX="376.0" layoutY="321.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="121.0" style="-fx-background-color: #FF8815;" text="Go!" textFill="#ffe7e7">
            <font>
                <Font name="Arial Bold" size="13.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <ImageView fx:id="back" fitHeight="71.0" fitWidth="37.0" onMouseClicked="#setBack" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../download.png" />
            </image>
        </ImageView>
      <TextField fx:id="tf" layoutX="266.0" layoutY="274.0" prefHeight="25.0" prefWidth="341.0" promptText="write down everything..">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
    </children>
</AnchorPane>
